{
	"name": "miemo-webapp",
	"version": "0.0.0",
	"scripts": {
		"dev": "vite",
		"test": "react-scripts test --watchAll=false",
		"build": "vite build ${@}",
		"serve": "vite preview",
		"lint": "eslint --ext .ts,.js,.jsx --ignore-path .gitignore .",
		"format": "prettier --write \"**/*.+(js|jsx|json|css|scss|md)\"",
		"lint-check": "eslint .",
		"extract-translations": "i18next --fail-on-warnings",
		"prepare": "cd .. && husky install \"MiEmo Webapp/.husky\""
	},
	"lint-staged": {
		"*.{ts,js,jsx}": "eslint --fix",
		"*": [
			"prettier --check "
		]
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"dependencies": {
		"@bybas/weather-icons": "^2.0.0",
		"@fullcalendar/bootstrap": "^5.10.1",
		"@fullcalendar/daygrid": "^5.10.1",
		"@fullcalendar/google-calendar": "^5.10.1",
		"@fullcalendar/interaction": "^5.10.1",
		"@fullcalendar/list": "^5.10.1",
		"@fullcalendar/react": "^5.10.1",
		"@fullcalendar/timegrid": "^5.10.1",
		"bootstrap": "^5.1.3",
		"bootstrap-dark-5": "^1.1.3",
		"chart.js": "^3.7.0",
		"dayjs": "^1.10.7",
		"events": "^3.3.0",
		"fetch-retry": "^5.0.3",
		"joypad.js": "^2.3.2",
		"lodash": "^4.17.21",
		"nanoid": "^4.0.0",
		"primeicons": "^6.0.1",
		"primereact": "^8.6.1",
		"rc-slider": "^10.0.1",
		"react": "^18.2.0",
		"react-audio-player": "^0.17.0",
		"react-bootstrap": "^2.1.0",
		"react-bootstrap-input-spinner": "^1.0.2",
		"react-bootstrap-range-slider": "^3.0.4",
		"react-clock": "^3.0.0",
		"react-colorful": "^5.5.1",
		"react-dom": "^18.2.0",
		"react-grid-layout": "^1.3.0",
		"react-howler": "^5.2.0",
		"react-icons": "^4.3.1",
		"react-router-dom": "^6.0.2",
		"react-simple-keyboard": "^3.4.234",
		"react-sketch-canvas": "^6.1.5",
		"react-transition-group": "^4.4.2",
		"react-webcam": "^7.0.1",
		"react-youtube": "^10.0.0",
		"styled-components": "^5.3.3",
		"x-react-flipclock": "^0.1.184153"
	},
	"devDependencies": {
		"@testing-library/jest-dom": "^5.14.1",
		"@testing-library/react": "^13.4.0",
		"@testing-library/user-event": "^14.4.3",
		"@vitejs/plugin-react": "^2.1.0",
		"enzyme": "^3.11.0",
		"enzyme-adapter-react-16": "^1.15.6",
		"eslint": "^8.2.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-prettier": "^4.0.0",
		"eslint-plugin-react": "^7.26.1",
		"flag-icons": "^6.0.2",
		"husky": "^8.0.1",
		"lint-staged": "^13.0.3",
		"prettier": "^2.4.1",
		"react-scripts": "^5.0.1",
		"vite": "^3.1.8"
	}
}
