{
	"name": "miemo-webapp",
	"version": "0.0.0",
	"scripts": {
		"dev": "vite",
		"test": "echo No Tests",
		"build": "vite build",
		"serve": "vite preview",
		"lint": "eslint --ext .ts,.js,.jsx --ignore-path .gitignore .",
		"format": "prettier --write \"**/*.+(js|jsx|json|css|scss|md)\"",
		"lint-check": "eslint .",
		"extract-translations": "i18next --fail-on-warnings",
		"prepare": "cd .. && husky install \"MiEmo Webapp/.husky\""
	},
	"lint-staged": {
		"*.{ts,js,jsx}": "eslint --fix",
		"*": [
			"prettier --check "
		]
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"dependencies": {
		"@bybas/weather-icons": "^2.0.0",
		"bootstrap": "^5.1.3",
		"dayjs": "^1.10.7",
		"primereact": "^7.0.1",
		"react": "^17.0.0",
		"react-bootstrap": "^2.1.0",
		"react-dom": "^17.0.0",
		"react-grid-layout": "^1.3.0",
		"react-icons": "^4.3.1",
		"react-router-dom": "^6.0.2",
		"styled-components": "^5.3.3"
	},
	"devDependencies": {
		"@vitejs/plugin-react": "^1.0.0",
		"eslint": "^8.2.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-prettier": "^4.0.0",
		"eslint-plugin-react": "^7.26.1",
		"flag-icons": "^6.0.2",
		"husky": "^7.0.0",
		"lint-staged": "^11.2.6",
		"prettier": "^2.4.1",
		"vite": "^2.6.4"
	}
}
