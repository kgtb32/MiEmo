		"x-react-flipclock": "^0.1.184153"
	},
	"devDependencies": {
		"@vitejs/plugin-react": "^1.0.0",
		"eslint": "^8.2.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-prettier": "^4.0.0",
		"eslint-plugin-react": "^7.26.1",
		"flag-icons": "^6.0.2",
		"husky": "^7.0.0",
		"lint-staged": "^11.2.6",
		"prettier": "^2.4.1",
		"vite": "^2.6.4"
	}
}
